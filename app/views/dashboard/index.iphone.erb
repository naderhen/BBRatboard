
<div id="home">
  <div class="toolbar">
    <h1>Current Inventory</h1>
  </div>
  <ul>
	<% for warehouse in @ratboard.warehouses  do %>
		<li><%= warehouse.name%></li>
  		<% for ratgrade in warehouse.ratgrades do %>
			<li class="arrow"><%= link_to ratgrade.name, ratgrade %> <small class="counter"><%= ratgrade.availableamt%> </small></li>
		<% end %>

	<% end %>
  </ul>
</div>
